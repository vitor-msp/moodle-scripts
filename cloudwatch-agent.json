{
    "agent": {
        "run_as_user": "root"
    },
    "logs": {
        "logs_collected": {
            "files": {
                "collect_list": [
                    {
                        "file_path": "/var/log/nginx/access.log",
                        "log_group_class": "STANDARD",
                        "log_group_name": "moodle-lg",
                        "log_stream_name": "nginx-access",
                        "retention_in_days": 7
                    },
                    {
                        "file_path": "/var/log/nginx/error.log",
                        "log_group_class": "STANDARD",
                        "log_group_name": "moodle-lg",
                        "log_stream_name": "nginx-error",
                        "retention_in_days": 7
                    }
                ]
            }
        }
    }
}